<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HabitatReStoreMobile.Pages.VolunteerHomePage"
             xmlns:vm="clr-namespace:HabitatReStoreMobile.ViewModels"
             Title="Volunteer">
    <ContentPage.BindingContext>
        <vm:VolunteerHomeViewModel/>
    </ContentPage.BindingContext>
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Go to Volunteer Sign Up" Command="{Binding SignUpCommand}" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>