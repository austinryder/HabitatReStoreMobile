<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HabitatReStoreMobile.Pages.FindReStorePage"
             xmlns:map="clr-namespace:HabitatReStoreMobile.Models"
             Title="Find a ReStore">
    <ContentPage.Content>
        <map:BindingMap MapType="Street" MapPosition="{Binding CurrentPosition}" MapPins="{Binding Pins}"/>
    </ContentPage.Content>
</ContentPage>