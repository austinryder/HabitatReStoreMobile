<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HabitatReStoreMobile.Pages.DonationHomePage"
             Title="Donate">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Donate!"/>
            <Button x:Name="btnGoPickupForm" Text="Sign Up for Donation Pickup" Clicked="btnGoPickupForm_Clicked" />
            <Button x:Name="btnGoDonorForm" Text="Enter Your Information" Clicked="btnGoDonorForm_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>